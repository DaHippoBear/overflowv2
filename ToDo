package com.jacob;

public class ToDo {

    private String title;
    private String description;
    private String time;

    public ToDo(String title, String description, String time){
        this.title = title;
        this.description = description;
        this.time = time;
    }

    public void set(String title, String description, String time){
        this.title = title;
        this.description = description;
        this.time = time;
    }

    public void getEvent(){

        System.out.println(getTitle() + ":" + getDescription() + ":" + getTime());
    }

    public String getTitle(){
        return this.title;
    }

    public String getDescription(){
        return this.description;
    }

    public String getTime(){
        return this.time;
    }


}
